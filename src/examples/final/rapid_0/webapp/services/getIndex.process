<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.hp.com/labs/rest/RESTTypes">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.hp.com/labs/rest/RESTTypes" schemaLocation="/REST/RESTTypes.xsd"/>
    <pd:name>webapp/services/getIndex.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:RestProcessInput"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx:RestProcessOutput>
            <pfx:extraHTTPHeaders>
                <xsl:copy-of select="$GetCookiesHeaders/pfx:ExtraHTTPHeaders/pfx:header"/>
            </pfx:extraHTTPHeaders>
        </pfx:RestProcessOutput>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:RestProcessOutput"/>
    <pd:endX>450</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1378232227414</pd:targetNamespace>
    <pd:activity name="GetCookiesHeaders">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>299</pd:x>
        <pd:y>52</pd:y>
        <config>
            <processName>/REST/Cookies/GetCookiesHeaders.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:Cookies>
                <pfx:cookie>
                    <pfx:name>
                        <xsl:value-of select="'peso'"/>
                    </pfx:name>
                    <pfx:value>
                        <xsl:value-of select="'gordo'"/>
                    </pfx:value>
                    <pfx:domain>
                        <xsl:value-of select="'mcserver'"/>
                    </pfx:domain>
                    <pfx:expires>
                        <xsl:value-of select="tib:add-to-dateTime(current-dateTime(), 0, 0, 0, 0, 0, 60)"/>
                    </pfx:expires>
                </pfx:cookie>
            </pfx:Cookies>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetCookiesHeaders</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetCookiesHeaders</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>