<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://www.hp.com/labs/rest/RESTTypes">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.hp.com/labs/rest/RESTTypes" schemaLocation="/REST/RESTTypes.xsd"/>
    <pd:name>webapp/services/postLogin.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:RestProcessInput"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx:RestProcessOutput>
            <pfx:extraHTTPHeaders>
                <xsl:copy-of select="$GetCookiesHeaders/pfx:ExtraHTTPHeaders/pfx:header"/>
            </pfx:extraHTTPHeaders>
            <pfx:body>
                <pfx:content>
                    <xsl:value-of select="'{}'"/>
                </pfx:content>
            </pfx:body>
        </pfx:RestProcessOutput>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:RestProcessOutput"/>
    <pd:endX>450</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1378232227414</pd:targetNamespace>
    <pd:activity name="GetCookiesHeaders">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>303</pd:x>
        <pd:y>64</pd:y>
        <config>
            <processName>/REST/Cookies/GetCookiesHeaders.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:Cookies>
                <pfx:cookie>
                    <pfx:name>
                        <xsl:value-of select="'token'"/>
                    </pfx:name>
                    <pfx:value>
                        <xsl:value-of select="'example'"/>
                    </pfx:value>
                    <pfx:max-age>
                        <xsl:value-of select="60*60"/>
                    </pfx:max-age>
                </pfx:cookie>
            </pfx:Cookies>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>265</pd:x>
        <pd:y>179</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="'Login failed'"/>
                </message>
                <messageCode>
                    <xsl:value-of select="'Login failed'"/>
                </messageCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetCookiesHeaders</pd:to>
        <pd:xpathDescription>right login?</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($Start/pfx:RestProcessInput/pfx:parameters/pfx:parameter[pfx:name='username' and pfx:value='admin'])&gt;0
and
count($Start/pfx:RestProcessInput/pfx:parameters/pfx:parameter[pfx:name='password' and pfx:value='admin'])&gt;0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>GetCookiesHeaders</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>